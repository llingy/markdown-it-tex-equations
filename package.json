    "math",
    "LaTeX",
    "markdown-it-plugin",
    "markdown-it"
  ],
  "author": "llingy",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "markdown-it": "^13.0.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:code && npm run build:type",
    "build:code": "tsc",
    "build:type": "tsc -d"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
